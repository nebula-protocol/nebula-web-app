{
  "name": "@nebula-web-app/app",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "app:dev": "vite",
    "app:dev:browser": "HTTPS=true PORT=3000 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts",
    "start": "multiplerun dev",
    "build": "tsc --noEmit && vite build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "react-scripts test --colors",
    "coverage": "react-scripts test --colors --coverage",
    "precommit": "lint-staged"
  },
  "multiplerun": {
    "dev": [
      "yarn run tsc --watch --noEmit",
      [
        "yarn run app:dev",
        {
          "command": "yarn run app:dev:browser",
          "wait": "5000"
        }
      ]
    ]
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "cross-env CI=true react-scripts test --colors --findRelatedTests"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@rx-stream/pipe": "^0.7.1",
    "@rx-stream/react": "^0.7.1",
    "@sentry/react": "^6.12.0",
    "@terra-money/terra.js": "^3.0.1",
    "@terra-money/use-wallet": "^3.0.0",
    "@terra-money/wallet-provider": "^3.0.0",
    "big.js": "^6.1.1",
    "buffer": "^6.0.3",
    "chart.js": "^3.5.1",
    "chartjs-plugin-datalabels": "^2.0.0",
    "color": "^4.0.1",
    "d3-ease": "^2.0.0",
    "d3-timer": "^2.0.0",
    "date-fns": "^2.23.0",
    "decimal.js": "^10.3.1",
    "fix-hmr": "^1.0.2",
    "graphql": "^15.5.2",
    "numeral": "^2.0.6",
    "puppeteer": "^10.4.0",
    "react": "^17.0.2",
    "react-csv": "^2.0.3",
    "react-dom": "^17.0.2",
    "react-masonry-css": "^1.0.16",
    "react-query": "^3.21.1",
    "react-responsive": "^9.0.0-beta.4",
    "react-router-dom": "^5.3.0",
    "react-spinners-kit": "^1.9.1",
    "react-use-clipboard": "^1.0.7",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.3.0",
    "styled-components": "^5.3.1",
    "use-resize-observer": "^8.0.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@peculiar/webcrypto": "^1.1.7",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@storybook/theming": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/big.js": "^6.1.2",
    "@types/color": "^3.0.2",
    "@types/d3-ease": "^3.0.0",
    "@types/d3-timer": "^3.0.0",
    "@types/glob": "^7.1.4",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.7.10",
    "@types/numeral": "^2.0.1",
    "@types/puppeteer": "^5.4.4",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.19",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-is": "^17.0.2",
    "@types/react-responsive": "^8.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.13",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__user-event": "^4.2.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "glob": "^7.1.7",
    "lint-staged": "^11.1.2",
    "multiplerun": "^2.0.1",
    "react-scripts": "^4.0.3",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vite-compatible-readable-stream": "^3.6.0",
    "vite-plugin-svgr": "^0.4.0",
    "vite-tsconfig-paths": "^3.3.13"
  }
}
